{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "^/(.*\\.js)$",
            "dest": "/public/$1"
        },
        {
            "src": "^/(.*\\.css)$",
            "dest": "/public/$1"
        },
        {
            "src": "^/(.*\\.ico)$",
            "dest": "/public/$1"
        },
        {
            "src": "^/img/(.*)$",
            "dest": "/public/img/$1"
        },
        {
            "src": "^/api/(.*)$",
            "dest": "/api/index.php"
        },
        {
            "src": "^/(.*)$",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://skylist-api.vercel.app/",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "SESSION_DRIVER": "cookie",
        "DB_CONNECTION": "pgsql",
        "DB_HOST": "ep-bold-sound-a4sn6hgj-pooler.us-east-1.aws.neon.tech",
        "DB_PORT": "5432",
        "DB_DATABASE": "neondb",
        "DB_USERNAME": "neondb_owner",
        "DB_PASSWORD": "npg_AOxT3Ryt6DKa",
        "DATABASE_URL": "postgres://neondb_owner:npg_AOxT3Ryt6DKa@ep-bold-sound-a4sn6hgj-pooler.us-east-1.aws.neon.tech/neondb?sslmode=require"
    },
    "outputDirectory": "public"
}